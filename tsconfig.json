{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "types": ["@figma/plugin-typings", "node"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}